{
	"name": "encase/framework",
	"description": "Just another PHP framework.",
	"license": "MIT",
	"authors": [
		{
			"name": "Deji69",
			"email": "the_zone@hotmail.co.uk"
		}
	],
	"require": {
		"php": "^7.2",
		"ext-mbstring": "*"
	},
	"require-dev": {
		"mockery/mockery": "^1.2",
		"phpbench/phpbench": "@dev",
		"phpunit/phpunit": "^8.1"
	},
	"autoload": {
		"psr-4": {
			"Encase\\": "src/Encase/"
		},
		"files": [
			"src/Encase/Functional/accumulate.php",
			"src/Encase/Functional/apply.php",
			"src/Encase/Functional/assertType.php",
			"src/Encase/Functional/box.php",
			"src/Encase/Functional/concat.php",
			"src/Encase/Functional/each.php",
			"src/Encase/Functional/fill.php",
			"src/Encase/Functional/find.php",
			"src/Encase/Functional/first.php",
			"src/Encase/Functional/isAssociativeArray.php",
			"src/Encase/Functional/isIndexedArray.php",
			"src/Encase/Functional/isSequentialArray.php",
			"src/Encase/Functional/isType.php",
			"src/Encase/Functional/join.php",
			"src/Encase/Functional/last.php",
			"src/Encase/Functional/map.php",
			"src/Encase/Functional/pop.php",
			"src/Encase/Functional/shift.php",
			"src/Encase/Functional/size.php",
			"src/Encase/Functional/slice.php",
			"src/Encase/Functional/split.php",
			"src/Encase/Functional/take.php",
			"src/Encase/Functional/toArray.php",
			"src/Encase/Functional/typeOf.php",
			"src/Encase/Functional/union.php",
			"src/Encase/Functional/unique.php",
			"src/Encase/Functional/values.php",
			"src/Encase/Matching/Support/_.php",
			"src/Encase/Matching/Support/match.php",
			"src/Encase/Matching/Support/pattern.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Encase\\Performance\\": "tests/Performance",
			"Encase\\Tests\\": "tests/Tests"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
