{
	"name": "deji/encase",
	"description": "Just another PHP framework.",
	"license": "MIT",
	"authors": [
		{
			"name": "Deji69",
			"email": "the_zone@hotmail.co.uk"
		}
	],
	"require": {
		"php": "^7.2"
	},
	"require-dev": {
		"mockery/mockery": "^1.2",
		"phpunit/phpunit": "^8.1"
	},
	"autoload": {
		"psr-4": {
			"Encase\\": "src/Encase/"
		},
		"files": [
			"src/Encase/Functional/apply.php",
			"src/Encase/Functional/assertType.php",
			"src/Encase/Functional/concat.php",
			"src/Encase/Functional/each.php",
			"src/Encase/Functional/find.php",
			"src/Encase/Functional/first.php",
			"src/Encase/Functional/isType.php",
			"src/Encase/Functional/join.php",
			"src/Encase/Functional/last.php",
			"src/Encase/Functional/map.php",
			"src/Encase/Functional/pop.php",
			"src/Encase/Functional/shift.php",
			"src/Encase/Functional/size.php",
			"src/Encase/Functional/slice.php",
			"src/Encase/Functional/split.php",
			"src/Encase/Functional/type.php",
			"src/Encase/Functional/values.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Encase\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
